{
  "name": "nextjs-start",
  "version": "0.0.1",
  "private": true,
  "author": {
    "name": "zbf",
    "email": "zbfcqtl@163.copm",
    "url": "https://zhoubangfu.com"
  },
  "keywords": [
    "antd",
    "next",
    "react",
    "seo",
    "theme",
    "less",
    "sass"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development PORT=3344 node ./scripts/run",
    "pro": "npm run build && cross-env NODE_ENV=production PORT=4444 node ./scripts/run",
    "start": "cross-env NODE_ENV=production PORT=4444 node ./scripts/run",
    "build": "cross-env NODE_ENV=production next build"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "antd": "^4.15.4",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "antd-theme-webpack-plugin": "^1.3.9",
    "babel-plugin-import": "^1.13.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "eslint-config-alloy": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.3.1",
    "husky": "^6.0.0",
    "less": "3.11.1",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "0.8.0",
    "next": "10.0.5",
    "next-compose-plugins": "^2.2.1",
    "null-loader": "^4.0.1",
    "prettier": "^2.2.1",
    "sass": "^1.32.12",
    "typescript": "^4.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "**/*.{ts,tsx,md,json}": [
      "prettier --write"
    ]
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "style": true
        }
      ]
    ]
  }
}
